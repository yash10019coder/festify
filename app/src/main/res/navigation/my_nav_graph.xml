<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav_graph"
    app:startDestination="@id/eventsFragment">

    <fragment
        android:id="@+id/eventsFragment"
        android:name="com.iiitlucknow.android.festify.event.EventFragment"
        android:label="EventsFragment">
        <action
            android:id="@+id/action_eventsFragment_to_clickFragment"
            app:destination="@id/clickFragment" />

    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.iiitlucknow.android.festify.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

    </fragment>
    <fragment
        android:id="@+id/teamsFragment"
        android:name="com.iiitlucknow.android.festify.TeamsFragment"
        android:label="fragment_teams"
        tools:layout="@layout/fragment_teams" />
    <fragment
        android:id="@+id/clickFragment"
        android:name="com.iiitlucknow.android.festify.event.EventSubcategoryFragment"
        android:label="fragment_click"
        tools:layout="@layout/fragment_click">
        <argument
            android:name="event"
            app:argType="string" />
    </fragment>

</navigation>
